<h1>Add New Map</h1>
<form method="post" action="/create">
  <div class="form-group">
    <label class="col-md-2 control-label">Upper Right</label>
    <div class="col-md-10">
      <div class="col-md-6">
        <label class="col-md-4 control-label">Longitude</label>
        <div class="col-md-8">
          <input type="number" step="0.000001" name="upper_right_longitude" min="-180" max="180" value="" placeholder="Longitude" class="urlon form-control">
        </div>
      </div>
      <div class="col-md-6">
        <label class="col-md-4 control-label">Latitude</label>
        <div class="col-md-8">
          <input type="number" step="0.000001" name="upper_right_latitude"  min="-90" max="90" value="" placeholder="Latitude" class="urlat form-control">
        </div>
      </div>
    </div>
  </div>
  
  
  <div class="form-group">
    <label class="col-md-2 control-label">Lower Left</label>
    <div class="col-md-10">
      <div class="col-md-6">
        <label class="col-md-4 control-label">Longitude</label>
        <div class="col-md-8">
          <input type="number" step="0.000001" name="lower_left_longitude" min="-180" max="180" value="" placeholder="Longitude" class="urlon form-control">
        </div>
      </div>
      <div class="col-md-6">
        <label class="col-md-4 control-label">Latitude</label>
        <div class="col-md-8">
          <input type="number" step="0.000001" name="lower_left_latitude"  min="-90" max="90" value="" placeholder="Latitude" class="urlat form-control">
        </div>
      </div>
    </div>
  </div>
  
  <div class="form-group">
    <div class="col-md-12">
      <input type="submit" class="btn btn-success" value="Create">
    </div>
  </div>
</form>